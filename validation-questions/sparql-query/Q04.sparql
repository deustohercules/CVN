prefix roh: <http://w3id.org/roh#>
PREFIX foaf: <http://w3id.org/roh/mirror/foaf#>
PREFIX vivo: <http://w3id.org/roh/mirror/vivo#>

SELECT distinct ?project  ?title ?monetaryAmount ?role
 WHERE { 
    ?project a vivo:Project;
            roh:title ?title;
            vivo:relates ?role.
        ?funding a roh:Funding;
                 
                roh:supports ?project.
Optional{ ?funding roh:monetaryAmount ?monetaryAmount.}
  }
  